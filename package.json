{
  "name": "evernote",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "test framework for evernote",
  "main": "index.js",
  "scripts": {
    "allure:generate": "npx allure generate ./allure-results --clean",
    "allure:open": "npx allure open ./allure-report",
    "allure:serve": "npx allure serve",
    "allure-reports": "node_modules/.bin/allure generate ./reports/allure/allure-results/  -o ./reports/allure/allure-report/ --clean && allure open ./reports/allure/allure-report",
    "posttest": "npm run allure:generate",
    "allure": "allure serve reports/allure-results",
    "test": "cucumber-js"
  },
  "cucumber-html-reporter": {
    "jsonFolder": "cucumber-report",
    "reportFolder": "cucumber-report"
  },
  "author": "irina Menshova",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^9.5.1",
    "@playwright/test": "^1.37.1",
    "allure-commandline": "^2.24.0",
    "allure-playwright": "^2.8.1"
  },
  "dependencies": {
    "chai": "^4.3.8",
    "playwright-cucumber": "^1.0.15"
  }
}
